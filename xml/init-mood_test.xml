<?xml version="1.0" ?>
<rdbmsInit        xsi:noNamespaceSchemaLocation="rdbmsInit.xsd"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >

  <table name="Delegates">
    <forAll col="status" val="OK" />
    <row>
      <field col="name" val="generic" />
      <field col="description" 
         val="for backwards compatibility, or when delegate does not matter" />
    </row>
    <row>
      <field col="name" val="ACD" />
      <field col="description" 
         val="owns ACD thresholds, masking, calib scripts.." />
    </row>
    <row>
      <field col="name" val="CAL" />
      <field col="description" 
         val="owns CAL thresholds, masking, calib scripts.." />
    </row>
    <row>
      <field col="name" val="TKR" />
      <field col="description" 
         val="owns TKR thresholds, masking, calib scripts.." />
    </row>
    <row>
      <field col="name" val="TRG" />
      <field col="description" 
         val="owns (LATC) GEM configuration" />
    </row>
    <row>
      <field col="name" val="TIM" />
      <field col="description" 
         val="owns (LATC) config of various delays" />
    </row>
    <row>
      <field col="name" val="HSK" />
      <field col="description" 
         val="owns thermal control parameters" />
    </row>
  </table>

  <table name="Conditions">
    <row>
      <field col="name" val="dont_care" />
      <field col="status" val="OK" />
      <field col="description" 
        val="when conditions are irrelevant and for testing" />
      <ref col="delegate_fk" table="Delegates"  refCol="name" 
         refVal="generic" />
    </row>
  </table>

